{
  "name": "web-js",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.3.15",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.9",
    "@mui/material": "^5.11.9",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "bson": "^5.0.1",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "graphql": "^16.6.0",
    "jwt-decode": "^3.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "realm-web": "^2.0.0",
    "stream-browserify": "^3.0.0",
    "tss-react": "^4.6.0",
    "web-vitals": "^3.1.1"
  },
  "scripts": {
    "start": "vite",
    "start:graphql": "VITE_API_TYPE=graphql npm run start",
    "start:mql": "VITE_API_TYPE=mql npm run start",
    "start:data-api": "VITE_API_TYPE=data-api npm run start",
    "build": "vite build",
    "build:graphql": "VITE_API_TYPE=graphql npm run build",
    "build:mql": "VITE_API_TYPE=mql npm run build",
    "build:data-api": "VITE_API_TYPE=data-api npm run build",
    "preview": "vite preview",
    "preview:graphql": "VITE_API_TYPE=graphql npm run preview",
    "preview:mql": "VITE_API_TYPE=mql npm run preview",
    "preview:data-api": "VITE_API_TYPE=data-api npm run preview",
    "test": "vitest run",
    "test:graphql": "VITE_API_TYPE=graphql npm run test",
    "test:mql": "VITE_API_TYPE=mql npm run test",
    "test:data-api": "VITE_API_TYPE=data-api npm run test",
    "coverage": "vitest run --coverage",
    "coverage:graphql": "VITE_API_TYPE=graphql npm run coverage",
    "coverage:mql": "VITE_API_TYPE=mql npm run coverage",
    "coverage:data-api": "VITE_API_TYPE=data-api npm run coverage",
    "prettier": "npx prettier ./src --write"
  },
  "eslintConfig": {
    "extends": []
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.8",
    "c8": "^7.13.0",
    "jsdom": "^21.1.1",
    "rollup": "^3.20.2",
    "vite": "^4.2.1",
    "vite-plugin-top-level-await": "^1.3.0",
    "vitest": "^0.29.8"
  }
}
