{
    "type": "flexible",
    "state": "enabled",
    "development_mode_enabled": true,
    "service_name": "mongodb-atlas",
    "database_name": "Item",
    "is_recovery_mode_disabled": false,
    "permissions": {
        "rules": {},
        "defaultRoles": [
            {
                "name": "owner-write",
                "applyWhen": {},
                "read": {
                    "$or": [
                        {
                            "owner_id": "%%user.id"
                        },
                        {
                            "owner_id": {
                                "$in": [
                                    "%%user.custom_data.subscribedTo"
                                ]
                            }
                        }
                    ]
                },
                "write": {
                    "owner_id": "%%user.id"
                }
            }
        ]
    },
    "queryable_fields_names": [
        "owner_id",
        "subscribedTo"
    ]
}
